{
  "name": "moodtracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.ts,.tsx --fix --ignore-path .gitignore .",
    "check-types": "tsc",
    "prettier": "prettier \"**/*.+(js|json|ts|tsx)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.0",
    "@react-navigation/bottom-tabs": "6.2.0",
    "@react-navigation/native": "6.0.8",
    "date-fns": "2.28.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-native": "0.67.4",
    "react-native-gesture-handler": "2.3.2",
    "react-native-reanimated": "2.4.1",
    "react-native-safe-area-context": "4.1.2",
    "react-native-screens": "3.13.1",
    "react-native-splash-screen": "3.3.0",
    "react-native-svg": "12.3.0",
    "victory-native": "36.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "7.17.8",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.180",
    "@types/react-native": "0.67.3",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "26.6.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "jest": "26.6.3",
    "lint-staged": "^12.3.7",
    "metro-react-native-babel-preset": "0.66.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "4.6.2"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
